library(tidyverse)

saiscore <- function(fileAddress){
  x = read.csv(fileAddress, header = F, nrows = 1)[-1]
  reverseSai = c(1, 2, 5, 8, 10, 11, 15, 16, 19, 20)
  regularSai = 1:20
  regularSai = regularSai[!(regularSai %in% reverseSai)]
  sai = sum(x[regularSai]) + sum((5-x[reverseSai]))
  return(sai)
}